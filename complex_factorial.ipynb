   "source": [
    "from scipy.integrate import quad\n",
    "import numpy as np\n",
    "\n",
    "# citation: based on professor's slides\n",
    "\n",
    "def f(x):\n",
    "    return np.exp(-x) * np.cos(np.log(x))\n",
    "\n",
    "def g(x):\n",
    "    return np.exp(-x) * np.sin(np.log(x))\n",
    "\n",
    "a = quad(f,0,10000)[0]\n",
    "b = quad(g,0,10000)[0]\n",
    "\n",
    "fact = complex(a,b)\n",
    "\n",
    "print(fact)"
   ]
